{
  "name": "aws-nodejs-typescript",
  "version": "1.0.0",
  "description": "Serverless webpack example using Typescript",
  "main": "handler.js",
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "rimraf ./dist && tsc",
    "test": "mocha --reporter spec --compilers ts:ts-node/register 'src/test/**/*.test.ts'",
    "deploy": "serverless deploy",
    "lint": "tslint \"src/**/*.ts\" --force"
  },
  "dependencies": {
    "aws-sdk": "^2.192.0",
    "axios": "^0.17.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "0.0.31",
    "@types/aws-sdk": "^2.7.0",
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.1.2",
    "@types/es6-promise": "^3.3.0",
    "@types/mocha": "^2.2.48",
    "@types/node": "^8.0.57",
    "chai": "^4.1.2",
    "codelyzer": "^4.1.0",
    "mocha": "^5.0.0",
    "rimraf": "^2.6.2",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "typescript": "^2.7.1"
  },
